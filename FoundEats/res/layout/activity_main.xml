<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.arizona.foundeats.MainActivity" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Working Main Activity to access others" />

    <Button
        android:id="@+id/button_HowToPlay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:text="How to Play" />

    <Button
        android:id="@+id/button_TypeOfMeal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:text="Type Of Meal" />

    <Button
        android:id="@+id/button_MealPlan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Plan Meal" />

    <Button
        android:id="@+id/button_Achievements"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Achievements" />

    <Button
        android:id="@+id/button_Map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Map" />

    <Button
        android:id="@+id/button_Challenge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Challenge" />

    <Button
        android:id="@+id/button_DataExample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_DataExample" />

</LinearLayout>