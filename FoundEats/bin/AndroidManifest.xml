<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.arizona.foundeats"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChallengeActivity"
            android:label="@string/title_activity_challenge" >
        </activity>
        <activity
            android:name=".MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name=".TypeOfMealActivity"
            android:label="@string/title_activity_type_of_meal" >
        </activity>
        <activity
            android:name=".HowToPlayActivity"
            android:label="@string/title_activity_hot_to_play" >
        </activity>
        <activity
            android:name=".MakeMealActivity"
            android:label="@string/title_activity_make_meal" >
        </activity>
        <activity
            android:name=".AchievementsActivity"
            android:label="@string/title_activity_achievements" >
        </activity>
        <activity
            android:name=".FoodActivity"
            android:label="@string/title_activity_food" >
        </activity>
        <activity
            android:name=".ScoreActivity"
            android:label="@string/title_activity_score" >
        </activity>
        <activity
            android:name=".PickOldMealActivity"
            android:label="@string/title_activity_pick_old_meal" >
        </activity>
        <activity
            android:name=".MealPlanActivity"
            android:label="@string/title_activity_meal_plan" >
        </activity>
    </application>

</manifest>
